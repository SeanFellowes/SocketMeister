  </metadata>
  <files>
    <file src="content\CrossDomainPolicy.xml" target="content\CrossDomainPolicy.xml" />
    <file src="contentFiles\any\net35\CrossDomainPolicy.xml" target="contentFiles\any\net35\CrossDomainPolicy.xml" />
