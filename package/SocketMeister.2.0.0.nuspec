    <file src="lib\net35\SocketMeister.dll" target="lib\net35\SocketMeister.dll" />
    <file src="lib\net35\SocketMeister.xml" target="lib\net35\SocketMeister.xml" />
    <file src="lib\net40\SocketMeister.dll" target="lib\net40\SocketMeister.dll" />
    <file src="lib\net40\SocketMeister.xml" target="lib\net40\SocketMeister.xml" />
    <file src="lib\net45\SocketMeister.dll" target="lib\net45\SocketMeister.dll" />
    <file src="lib\net45\SocketMeister.xml" target="lib\net45\SocketMeister.xml" />
    <file src="lib\net472\SocketMeister.dll" target="lib\net472\SocketMeister.dll" />
    <file src="lib\net472\SocketMeister.xml" target="lib\net472\SocketMeister.xml" />
    <file src="lib\netstandard2.0\SocketMeister.dll" target="lib\netstandard2.0\SocketMeister.dll" />
    <file src="lib\netstandard2.0\SocketMeister.xml" target="lib\netstandard2.0\SocketMeister.xml" />
    <file src="icon_128.png" target="icon_128.png" />
