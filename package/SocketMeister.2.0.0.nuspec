<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>SocketMeister</id>
    <version>2.0.0</version>
    <authors>Sean Fellowes</authors>
    <owners>Sean Fellowes</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <icon>icon_128.png</icon>
    <projectUrl>https://github.com/SeanFellowes/SocketMeister</projectUrl>
    <description>Performant TCP socket server and client for .NET</description>
    <releaseNotes>Resolved fxCop issues</releaseNotes>
    <copyright>Copyright © 2010-2020 Sean Fellowes</copyright>
    <tags>tcp socket</tags>
    <repository type="git" url="https://github.com/SeanFellowes/SocketMeister" />
    <dependencies>
      <group targetFramework=".NETFramework3.5" />
      <group targetFramework=".NETFramework4.0" />
      <group targetFramework=".NETFramework4.5" />
      <group targetFramework=".NETFramework4.7.2" />
      <group targetFramework=".NETStandard2.0" />
    </dependencies>
    <contentFiles>
      <files include="any/net35/CrossDomainPolicy.xml" buildAction="Content" />
      <files include="any/net40/CrossDomainPolicy.xml" buildAction="Content" />
      <files include="any/net45/CrossDomainPolicy.xml" buildAction="Content" />
      <files include="any/net472/CrossDomainPolicy.xml" buildAction="Content" />
      <files include="any/netstandard2.0/CrossDomainPolicy.xml" buildAction="Content" />
